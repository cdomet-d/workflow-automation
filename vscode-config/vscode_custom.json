		"types": "#ee6055", // Bright orange (kept as is)
		"variables": "#427aa1", // Steel blue
		"functions": "#C1121F", // Deep red (kept as is)
		"strings": "#2A9D8F", // Teal
		"numbers": "#CA5B6F", // Rose (kept as is)
		"textMateRules": [
			{
				"scope": [
					"keyword"
				],
				"settings": {
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"entity.name.function"
				],
				"settings": {
					"foreground": "#da2c38",
					"fontStyle": ""
				}
			},
			{
				"scope": [
					"entity.name.class",
					"entity.name.type.class.templated.cpp"
				],
				"settings": {
					"fontStyle": "bold underline",
					"foreground": "#679436",
				}
			},
			{
				"scope": [
					"keyword.operator.logical.cpp"
				],
				"settings": {
					"fontStyle": "",
					"foreground": "#da2c38"
				}
			}
		]
	},
